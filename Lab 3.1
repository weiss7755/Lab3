using System;

class Animal
{
    public virtual void MakeSound()
    {
        Console.WriteLine("Звуки тварин");
    }
}

class Dog : Animal
{
    public override void MakeSound()
    {
        Console.WriteLine("Гав!");
    }
}

class Cat : Animal
{
    public override void MakeSound()
    {
        Console.WriteLine("Мяв!");
    }
}

class Program
{
    static void Main()
    {
        Animal dog = new Dog();
        Animal cat = new Cat();

        dog.MakeSound();  
        cat.MakeSound();  
    }
}
